{
  "name": "poinz",
  "version": "0.20.3",
  "description": "Distributed Planning Poker",
  "private": true,
  "author": "xeronimus@gmail.com",
  "license": "MIT",
  "homepage": "https://github.com/Zuehlke/poinz",
  "repository": {
    "type": "git",
    "url": "https://github.com/Zuehlke/poinz.git"
  },
  "scripts": {
    "ft": "nx exec -- npm run _ft -- ",
    "format": "nx exec -- npm run _format -- ",
    "test": "nx exec -- npm run _test -- ",
    "prebuild": "node build/checkTranslationKeys.mjs && npm run docu",
    "build": "nx exec -- node build/build.mjs",
    "build:dockerImage": "nx exec -- npm run _build:dockerImage -- ",
    "dependencyCheck": "nx exec -- npm run _dependencyCheck -- ",
    "postinstall": "cd client/ && npm i && cd ../server && npm i",
    "docu": "nx exec -- node docu/src/docuGenerator.mjs",
    "docu:diagrams": "nx exec -- node docu/src/docuGenerator.mjs -- diagrams",
    "_ft": "npm run format && npm t",
    "_format": "cd client/ && npm run format && cd ../server/ && npm run format  && cd ../ && prettier --write \"build/*.mjs\" \"docu/**/*.mjs\" ",
    "_test": "cd client/ && npm test &&  cd ../server && npm test",
    "_build:dockerImage": "npm run build && node build/dockerImage.mjs",
    "_dependencyCheck": "ncu --packageFile ./package.json > npm_dependencies_report.poinz.md && ncu --packageFile ./client/package.json > npm_dependencies_report.poinz-client.md && ncu --packageFile ./server/package.json > npm_dependencies_report.poinz-server.md "
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@eslint/js": "^9.8.0",
    "@mermaid-js/mermaid-cli": "^10.5.0",
    "@nx/eslint": "20.3.1",
    "@nx/eslint-plugin": "20.3.1",
    "@nx/js": "20.3.1",
    "@nx/react": "20.3.1",
    "@nx/vite": "20.3.1",
    "@nx/web": "20.3.1",
    "@swc-node/register": "~1.9.1",
    "@swc/cli": "~0.3.12",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@testing-library/react": "15.0.6",
    "@types/node": "18.16.9",
    "@types/react": "18.3.1",
    "@types/react-dom": "18.3.0",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/coverage-v8": "^1.0.4",
    "@vitest/ui": "^1.3.1",
    "autoprefixer": "10.4.13",
    "chalk": "^5.3.0",
    "cross-spawn": "7.0.3",
    "del": "7.1.0",
    "ejs": "^3.1.9",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jsx-a11y": "6.10.1",
    "eslint-plugin-react": "7.35.0",
    "eslint-plugin-react-hooks": "5.0.0",
    "fs-extra": "11.1.1",
    "glob": "^10.3.10",
    "jsdom": "~22.1.0",
    "npm-check-updates": "16.14.6",
    "nx": "20.3.1",
    "postcss": "8.4.38",
    "prettier": "3.0.3",
    "styled-components": "^6.1.14",
    "tailwindcss": "3.4.3",
    "tslib": "^2.3.0",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.13.0",
    "vite": "^5.0.0",
    "vitest": "^1.3.1"
  },
  "engines": {
    "node": ">=18"
  },
  "nx": {
    "targets": {
      "format": {
        "outputs": [
          "{projectRoot}/build",
          "{projectRoot}/docu"
        ]
      },
      "build": {
        "outputs": [
          "{projectRoot}/deploy"
        ]
      },
      "docu": {
        "outputs": [
          "{projectRoot}/docu"
        ]
      },
      "docu:diagrams": {
        "outputs": [
          "{projectRoot}/diagrams"
        ]
      }
    }
  },
  "dependencies": {
    "date-fns": "^4.1.0",
    "loglevel": "^1.9.2",
    "react": "18.3.1",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "18.3.1",
    "react-dropzone": "^14.3.5",
    "react-linkify": "^1.0.0-alpha",
    "react-markdown": "^9.0.3",
    "react-redux": "^9.2.0",
    "react-router-dom": "6.11.2",
    "redux": "^5.0.1",
    "redux-thunk": "^3.1.0",
    "remark-gfm": "^4.0.0",
    "reselect": "^5.1.1"
  }
}
